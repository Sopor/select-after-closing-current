{
	"manifest_version": 2,
	"applications": {
		"gecko": {
			"id": "select-after-closing-current@qw.linux-2g64.local"
		}
	},
	"name": "__MSG_extensionName__",
	"version": "2.0rc4",
	"description": "__MSG_extensionDescription__",
	"icons": {
		"48": "icons/icon.svg"
	},
	"permissions": [
		"tabs",
		"storage"
	],
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"options_ui": {
		"page": "options.html"
	},
	"default_locale": "en",
	"commands": {
		"close-1": {
			"suggested_key": {
				"default": "Ctrl+Shift+F4"
			},
			"description": "Close the current tab (alternatively)"
		}
	}
}